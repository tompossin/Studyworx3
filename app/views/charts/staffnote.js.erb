$('#staffnote').html("<%=j render "staffnotes/form" %>");
if($("#staffnote_<%=j @staffnote.id.to_s %>").length){
  $('#notice').show().html("Loading...").fadeOut(2000);
}else{
  $("#<%=j @staffnote.noteable_type %>_<%=j @staffnote.noteable_id.to_s %>").prepend("<%=j render "staffnotes/snicon" %>");
}



