  <div id='profile' onclick="ShowHide('profilemenu')">
      <%= full_name %>
    <p id="profilemenu" style= "display:none">
      <br />
      <% if current_user %>
        <% if session[:admin_level] == 3 %>
          <%= link_to "User Admin", admin_user_admins_path %> <br />
          <%= link_to "Schools Admin", admin_schools_path %> <br />
          <%= link_to "Blog Admin", blogs_path %> <br />
          <%= link_to "System Help Admin", admin_helps_path %><br />
          <%= link_to "Admin Tools", admin_tools_index_path %><br />
          <br />
        <% elsif session[:admin_level] == 2 %>
          <%= link_to "Schools Admin", admin_schools_path %> <br />
          <%= link_to "Blog Admin", blogs_path %> <br />
          <%= link_to "Admin Tools", admin_tools_index_path %><br />
          <br />
        <% elsif session[:admin_level] == 1 %>
          <%= link_to "Blog Admin", blogs_path %> <br />
          <br />
        <% elsif current_user.role < 3 %>
          <%= link_to "Admin Tools", admin_tools_index_path %><br />
          <br />
        <% end %>
      <%end%>
      <%= link_to "Profile Page", edit_profile_path(current_user.id) if current_user %><br />
      <%= link_to "Change Password", edit_user_registration_path if current_user %><br />
      <%= link_to "Log Out", destroy_user_session_path, :method=>:delete if current_user %><br />
    </p>
  </div>